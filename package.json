    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "husky": ">=9.0.11",
    "lint-staged": ">=15.2.2",
    "prettier": "3.2.5"
  },
  "lint-staged": {
    "*.js": "npm run format",
    "*.{js,css,md}": "prettier --write"
  }
